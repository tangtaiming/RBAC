<?xml version="1.0" encoding="UTF-8" ?>
<user class="access">
    <head>
        <title>access.management</title>
        <script src="admin/access.js" />
        <link href="admin/access.css" />
        <create link="/admin/createAccess" />
    </head>
    <body>
        <head-container>
            <column title="id" />
            <column title="title" />
            <column title="status"/>
            <column title="createDate" />
            <column title="action" />
        </head-container>
        <search-container>
            <column type="text" name="id" />
            <column type="text" name="title" />
            <column type="select" name="status" option="Status"/>
            <column type="text" name="createDate" />
            <column type="action" style="base">
                <param type="edit" link="/admin/editAccess" />
                <param type="delete" link="/admin/deleteAccess" />
            </column>
        </search-container>
        <data-container>
            <column type="text" name="id" />
            <column type="text" name="title" />
            <column type="text" name="status"/>
            <column type="text" name="createDate" />
            <column type="action" />
        </data-container>
    </body>
</user>